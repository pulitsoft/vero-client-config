{
    "compilerOptions": {
      "target": "es2020",
      "lib": [
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
    },
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  